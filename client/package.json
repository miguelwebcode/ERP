{
  "name": "vite-react-ts-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npx kill-port --port 5173 && vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "vitest": "vitest --ui",
    "emulators": "npx kill-port --port 4000,5000,5001,8080,9000,9099 && firebase emulators:start --import=backup-firebase",
    "cypress:run": "npx cypress run",
    "cypress:open": "powershell -Command \"$env:SERVICE_ACCOUNT = Get-Content -Path .\\serviceAccount.json -Raw; npx cypress open\"",
    "cypress:emulator": "cross-env FIREBASE_AUTH_EMULATOR_HOST=\"127.0.0.1:$(cat firebase.json | jq .emulators.auth.port)\"  FIRESTORE_EMULATOR_HOST=\"127.0.0.1:8080\" npm run cypress:open"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@firebase/rules-unit-testing": "^4.0.1",
    "@mui/material": "^7.0.1",
    "@radix-ui/react-dialog": "^1.1.10",
    "@radix-ui/react-separator": "^1.1.4",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-tooltip": "^1.2.3",
    "@types/js-cookie": "^3.0.6",
    "@vitejs/plugin-react": "^4.3.4",
    "axios": "^1.7.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "firebase": "^11.2.0",
    "firebase-admin": "^12.7.0",
    "formik": "^2.4.6",
    "fs": "^0.0.1-security",
    "js-cookie": "^3.0.5",
    "kill-port": "^2.0.1",
    "lucide-react": "^0.501.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.1.3",
    "react-toastify": "^11.0.5",
    "recharts": "^2.15.2",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.0.5",
    "vite-tsconfig-paths": "^5.1.4",
    "yup": "^1.6.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.4",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/ui": "^3.0.5",
    "autoprefixer": "^10.4.20",
    "cypress": "^14.1.0",
    "cypress-firebase": "^4.2.1",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "jsdom": "^26.0.0",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vitest": "^3.0.5"
  }
}
